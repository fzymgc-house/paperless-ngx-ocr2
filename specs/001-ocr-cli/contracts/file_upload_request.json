{
  "title": "Mistral AI File Upload Request Contract",
  "description": "Contract for uploading files to Mistral AI Files API",
  "version": "1.0.0",
  "api_version": "v1",
  "last_updated": "2025-01-26",
  "type": "object",
  "required": ["file"],
  "properties": {
    "file": {
      "type": "string",
      "format": "binary",
      "description": "The file to upload (multipart/form-data)"
    },
    "purpose": {
      "type": "string",
      "enum": ["ocr"],
      "description": "Purpose of the file upload",
      "default": "ocr"
    }
  }
}
